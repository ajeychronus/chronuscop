<h1><%= @project.name %></h1>
<hr>
<% if(@project.translations.count == 0) then %>
<%= "No translations added yet" %>
<% else %>

<h2>Translations list</h2>
<table border="1">
<% @project.translations.each do |t| %>
  <tr>
    <td><%= t.key %></td>
    <td><%= t.value %></td>
  </tr>
<% end %>
</table>

<% end %>
